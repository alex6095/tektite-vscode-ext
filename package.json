{
  "name": "tektite",
  "displayName": "Tektite - Code from the Stars",
  "description": "Think in Graphs, Write in Blocks. A Code Knowledge Graph visualizer for your codebase.",
  "version": "0.1.0",
  "publisher": "tektite",
  "repository": {
    "type": "git",
    "url": "https://github.com/alex6095/tektite-vscode-ext"
  },
  "engines": {
    "vscode": "^1.85.0"
  },
  "categories": [
    "Visualization",
    "Other"
  ],
  "keywords": [
    "graph",
    "visualization",
    "code",
    "knowledge graph",
    "python"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "tektite.openGraph",
        "title": "Tektite: Open Code Graph"
      },
      {
        "command": "tektite.setApiKey",
        "title": "Tektite: Set Gemini API Key"
      }
    ],
    "configuration": {
      "title": "Tektite",
      "properties": {
        "tektite.pythonPath": {
          "type": "string",
          "default": "python3",
          "description": "Path to Python interpreter for code execution"
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile && npm run build:webview",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "build:webview": "cd webview-ui && npm run build",
    "dev:webview": "cd webview-ui && npm run dev",
    "package": "vsce package"
  },
  "devDependencies": {
    "@types/vscode": "^1.85.0",
    "@types/node": "^20.0.0",
    "typescript": "^5.3.0",
    "@google/genai": "^1.33.0"
  },
  "dependencies": {
    "@google/genai": "^1.33.0"
  }
}